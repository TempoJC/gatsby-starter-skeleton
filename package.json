{
  "name": "gatsby-starter-skeleton",
  "version": "0.3.0",
  "private": true,
  "description": "gatsby-starter-skeleton",
  "author": "JoseCC",
  "keywords": [
    "gatsby",
    "typescript",
    "eslint",
    "husky",
    "prettier",
    "docker",
    "makefile",
    "stylelint",
    "gitlab",
    "tailwind",
    "jest"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "format:eslint": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "format": "npm run format:eslint && npm run format:prettier",
    "check:format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "check:lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --ext .js,.ts --color",
    "stylelint": "stylelint \"src/**/*.css\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test:unit": "jest --passWithNoTests",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "dependencies": {
    "@mdx-js/react": "^2.1.3",
    "gatsby": "^4.21.1",
    "gatsby-plugin-google-analytics": "^4.21.0",
    "gatsby-plugin-image": "^2.21.0",
    "gatsby-plugin-manifest": "^4.21.0",
    "gatsby-plugin-mdx": "^4.0.0",
    "gatsby-plugin-sharp": "^4.21.0",
    "gatsby-plugin-sitemap": "^5.21.0",
    "gatsby-source-filesystem": "^4.21.1",
    "gatsby-transformer-remark": "^5.21.0",
    "gatsby-transformer-sharp": "^4.21.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "autoprefixer": "^10.4.8",
    "babel-jest": "^29.0.2",
    "babel-preset-gatsby": "^2.22.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.0",
    "eslint-webpack-plugin": "^3.2.0",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-postcss": "^5.21.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  }
}
